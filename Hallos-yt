local UtilitiesSection = Utilities:NewSection("Stuff")


UtilitiesSection:NewButton("Fps count", "shows fps and ping", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/1201for/littlegui/main/FPS-Counter'))()
end)
